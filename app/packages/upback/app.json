{
  "expo": {
    "name": "expo-backups-demo",
    "slug": "expo-backups-demo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.timmyjose.expobackupsdemo",
      "entitlements": {
        "com.apple.developer.icloud-container-identifiers": [
          "iCloud.com.timmyjose.expobackupsdemo"
        ],
        "com.apple.developer.icloud-services": [
          "CloudKit",
          "CloudDocuments"
        ],
        "com.apple.developer.ubiquity-kvstore-identifier": "$(TeamIdentifierPrefix)$(CFBundleIdentifier)"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.timmyjose.expobackupsdemo"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "ae7ca831-ffda-4799-aec8-5ed50800d3c3"
      }
    },
    "owner": "timmyjose"
  }
}
